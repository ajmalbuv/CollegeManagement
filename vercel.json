{
  "version": 2,
  "builds": [
    {
      "src": "CollegeERP/wsgi.py",
      "use": "@ardnt/vercel-python-wsgi",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.9",
        "buildCommand": "python manage.py collectstatic --noinput && python manage.py migrate"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": " /static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "CollegeERP/wsgi.py"
    }
  ]
}
